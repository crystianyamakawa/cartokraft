<p id="notice"><%= notice %></p>

<h1>Despesas</h1>

<table>
  <thead>
    <tr>
      <th>Dt fechamento</th>
      <th>Nm despesa</th>
      <th>Tp despesa</th>
      <th>Dt despesa</th>
      <th>Vl despesa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @despesas.each do |despesa| %>
      <tr>
        <td><%= despesa.dt_fechamento %></td>
        <td><%= despesa.nm_despesa %></td>
        <td><%= despesa.tp_despesa %></td>
        <td><%= despesa.dt_despesa %></td>
        <td><%= despesa.vl_despesa %></td>
        <td><%= link_to 'Show', despesa %></td>
        <td><%= link_to 'Edit', edit_despesa_path(despesa) %></td>
        <td><%= link_to 'Destroy', despesa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Despesa', new_despesa_path %>
