<p id="notice"><%= notice %></p>

<h1>Materials</h1>

<table>
  <thead>
    <tr>
      <th>name</th>
      <th>Nr localizador</th>
      <th>Status</th>
      <th>Area</th>
      <th>Largura</th>
      <th>Comprimento</th>
      <th>Tp material</th>
      <th>Fornecedor</th>
      <th>Obs</th>
      <th>Created by</th>
      <th>Updated by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @materials.each do |material| %>
      <tr>
        <td><%= material.name %></td>
        <td><%= material.nr_localizador %></td>
        <td><%= material.status %></td>
        <td><%= material.area %></td>
        <td><%= material.largura %></td>
        <td><%= material.comprimento %></td>
        <td><%= material.tp_material %></td>
        <td><%= material.fornecedor_id %></td>
        <td><%= material.obs %></td>
        <td><%= material.created_by %></td>
        <td><%= material.updated_by %></td>
        <td><%= link_to 'Show', material %></td>
        <td><%= link_to 'Edit', edit_material_path(material) %></td>
        <td><%= link_to 'Destroy', material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Material', new_material_path %>
