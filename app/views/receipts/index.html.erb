<p id="notice"><%= notice %></p>

<h1>Receipts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Partner</th>
      <th>Entrega</th>
      <th>Amount scheduled</th>
      <th>Status</th>
      <th>Date scheduled</th>
      <th>Date executed</th>
      <th>Ammount executed</th>
      <th>Obs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @receipts.each do |receipt| %>
      <tr>
        <td><%= receipt.name %></td>
        <td><%= receipt.type %></td>
        <td><%= receipt.partner %></td>
        <td><%= receipt.entrega %></td>
        <td><%= receipt.amount_scheduled %></td>
        <td><%= receipt.status %></td>
        <td><%= receipt.date_scheduled %></td>
        <td><%= receipt.date_executed %></td>
        <td><%= receipt.ammount_executed %></td>
        <td><%= receipt.obs %></td>
        <td><%= link_to 'Show', receipt %></td>
        <td><%= link_to 'Edit', edit_receipt_path(receipt) %></td>
        <td><%= link_to 'Destroy', receipt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Receipt', new_receipt_path %>
